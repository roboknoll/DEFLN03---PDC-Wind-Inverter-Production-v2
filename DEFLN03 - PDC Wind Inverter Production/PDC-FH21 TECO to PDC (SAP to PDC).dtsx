              DTS:ExecutableType="Microsoft.ExpressionTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Convert TimeStamp to String"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <ExpressionTask
                  Expression="@[User::H01_RecTimeStamp] = (DT_WSTR, 4)DATEPART(&quot;year&quot;,@[User::H01_RecTimeStamp_DateTime]) + (DT_WSTR, 2)RIGHT( &quot;00&quot;+(DT_WSTR, 2) DATEPART(&quot;month&quot;,@[User::H01_RecTimeStamp_DateTime]),2) + (DT_WSTR, 2)RIGHT( &quot;00&quot;+(DT_WSTR, 2) DATEPART(&quot;day&quot;,@[User::H01_RecTimeStamp_DateTime]),2) + (DT_WSTR, 2)RIGHT( &quot;00&quot;+(DT_WSTR, 2) DATEPART(&quot;hour&quot;,@[User::H01_RecTimeStamp_DateTime]),2) + (DT_WSTR, 2)RIGHT( &quot;00&quot;+(DT_WSTR, 2) DATEPART(&quot;minute&quot;,@[User::H01_RecTimeStamp_DateTime]),2) + (DT_WSTR, 2)RIGHT( &quot;00&quot;+(DT_WSTR, 2) DATEPART(&quot;second&quot;,@[User::H01_RecTimeStamp_DateTime]),2)" />
              </DTS:ObjectData>
            </DTS:Executable>
            <DTS:Executable
              DTS:refId="Package\PDC-FH21\Production Order Loop\Create Header 01"
              DTS:CreationName="Microsoft.ExpressionTask"
              DTS:Description="Expression Task"
              DTS:DTSID="{FDDC6C2F-21B1-4B10-9A61-AF43A1F70EE3}"
              DTS:ExecutableType="Microsoft.ExpressionTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Create Header 01"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <ExpressionTask
                  Expression="@[User::Header01] = &quot;01&quot; +  LEFT(@[User::H01_ProductionOrderNo] + REPLICATE(&quot; &quot;,8),8) + LEFT(@[User::H01_TecoMark] + &quot; &quot;,1) + LEFT(@[User::H01_RecTimeStamp] + REPLICATE(&quot; &quot;,14),14) + &quot;\n&quot;" />
